{
  "name": "velos_iam",
  "version": "1.0.0",
  "description": "User Management",
  "main": "index.js",
  "scripts": {
    "test": "tap debug --node-arg=--loader=ts-node/esm --node-arg=--require=./test/setup.ts  --timeout=0 \"test/**/*.test.ts\" \"src/**/*test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js -p 8001",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js -p 8001"
  },
  "keywords": [
    "User",
    "management",
    "Identity",
    "access",
    "management"
  ],
  "author": "sundarave;l",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/multipart": "^9.0.1",
    "@fastify/type-provider-typebox": "^5.0.0",
    "@sinclair/typebox": "^0.33.16",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-autoload": "^3.4.1",
    "module-alias": "^2.2.3",
    "mongoose": "^8.7.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "concurrently": "^9.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
